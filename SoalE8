 boolean binarySearch(int[] x, int start, int end, int n) {
        if (end < start) return false;
        int mid = (start+end) / 2;
        if (x[mid] == n) {
            return true;
        } else {
            if (x[mid] < n) {
                return search(x, mid+1, end, n);
            } else {
                return search(x, start, mid-1, n);
            }
        }
    }
}
